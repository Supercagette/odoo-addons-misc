<?xml version="1.0" encoding="utf-8"?>
<templates id="template" xml:space="preserve">

    <!-- Overload 'PosWidget' -->
    <t t-extend="PosWidget">
        <t t-jquery=".pos-logo" t-operation="append">
            <span id="button_market_place" class="order-button">Market Place</span>
        </t>
    </t>

    <!-- Define 'SelectMarketPlacePopupWidget' -->
    <t t-name="SelectMarketPlacePopupWidget">
        <div class="modal-dialog">
            <div class="popup popup-select-variant">
                <div class="variant-title">
                    Market Place Selection
                    <span id="variant-popup-cancel" class="button">
                        Do not Select Market Place
                    </span>
                    
                </div>
            </div>
        </div>
    </t>

